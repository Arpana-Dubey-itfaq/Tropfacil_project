<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:cardView="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F6F6FB"
    android:backgroundTint="#F6F6FB">

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="match_parent"
        android:layout_height="98dp"
        android:background="#E83C70"

        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Account Settings"
        android:textColor="#FFFFFF"
        android:textSize="18dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="110dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:orientation="vertical"
            android:padding="5dp"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView23">

            <TextView
                android:id="@+id/textView23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:padding="10dp"
                android:text="Account Settings"
                android:textColor="#000000"
                android:textSize="24dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@id/textView2" />

            <androidx.cardview.widget.CardView
                android:id="@+id/text"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_margin="7dp"
                cardView:cardCornerRadius="15dp"
                cardView:cardElevation="0dp"
                tools:ignore="MissingConstraints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/Changeemail1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dim20"
                        android:scaleType="centerCrop"
                        android:src="@drawable/go_arrow_green_1"
                        app:layout_constraintEnd_toEndOf="parent"
                        cardView:layout_constraintBottom_toBottomOf="parent"
                        cardView:layout_constraintLeft_toRightOf="@id/Changeemail"
                        cardView:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/Changeemail"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:gravity="center_vertical"
                        android:lines="1"
                        android:padding="5dp"
                        android:text="Change email"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        cardView:layout_constraintStart_toStartOf="parent"
                        cardView:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/text2"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_margin="7dp"
                cardView:cardCornerRadius="15dp"
                cardView:cardElevation="0dp"
                tools:ignore="MissingConstraints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/Changepassword1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dim20"
                        android:scaleType="centerCrop"
                        android:src="@drawable/go_arrow_green_1"
                        app:layout_constraintEnd_toEndOf="parent"
                        cardView:layout_constraintBottom_toBottomOf="parent"
                        cardView:layout_constraintLeft_toRightOf="@id/Changepassword"
                        cardView:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/Changepassword"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:gravity="center_vertical"
                        android:lines="1"
                        android:padding="5dp"
                        android:text="Change password"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        cardView:layout_constraintStart_toStartOf="parent"
                        cardView:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/text4"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_margin="7dp"
                cardView:cardCornerRadius="15dp"
                cardView:cardElevation="0dp"
                android:visibility="gone"
                tools:ignore="MissingConstraints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:visibility="gone"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/SavedCards"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dim20"
                        android:scaleType="centerCrop"
                        android:src="@drawable/go_arrow_green_1"
                        app:layout_constraintEnd_toEndOf="parent"
                        cardView:layout_constraintBottom_toBottomOf="parent"


                        cardView:layout_constraintLeft_toRightOf="@id/SavedCards1"
                        cardView:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/SavedCards1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:gravity="center_vertical"
                        android:lines="1"
                        android:padding="5dp"
                        android:text="Saved Cards"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        cardView:layout_constraintStart_toStartOf="parent"
                        cardView:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/text5"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_margin="7dp"
                cardView:cardCornerRadius="15dp"
                cardView:cardElevation="0dp"
                android:visibility="gone"
                tools:ignore="MissingConstraints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/Subscription"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dim20"
                        android:scaleType="centerCrop"
                        android:src="@drawable/go_arrow_green_1"
                        app:layout_constraintEnd_toEndOf="parent"
                        cardView:layout_constraintBottom_toBottomOf="parent"


                        cardView:layout_constraintLeft_toRightOf="@id/Subscription1"
                        cardView:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/Subscription1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:gravity="center_vertical"
                        android:lines="1"
                        android:padding="5dp"
                        android:text="Subscription"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        cardView:layout_constraintStart_toStartOf="parent"
                        cardView:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/text6"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_margin="7dp"
                cardView:cardCornerRadius="15dp"
                cardView:cardElevation="0dp"
                tools:ignore="MissingConstraints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/Synchronization"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dim20"
                        android:scaleType="centerCrop"
                        android:src="@drawable/account_settings_sync"
                        app:layout_constraintEnd_toEndOf="parent"
                        cardView:layout_constraintBottom_toBottomOf="parent"


                        cardView:layout_constraintLeft_toRightOf="@id/Synchronization"
                        cardView:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/Synchronization1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:lines="1"
                        android:padding="5dp"
                        android:text="Synchronization"
                        android:textColor="#000000"
                        android:textStyle="bold"


                        cardView:layout_constraintStart_toStartOf="parent"
                        cardView:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/textView10"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_marginTop="21dp"
                        android:layout_marginRight="48dp"
                        android:text="Sync Now"
                        android:textColor="#38B6A4"
                        android:textSize="14dp"
                        app:layout_constraintRight_toRightOf="@+id/Synchronization1"
                        app:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/text7"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                android:layout_marginTop="24dp"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                cardView:cardCornerRadius="15dp"
                cardView:cardElevation="0dp"
                tools:ignore="MissingConstraints">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clipToPadding="true"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp">

                    <ImageView
                        android:id="@+id/Synchronization3"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/dim20"
                        android:scaleType="centerCrop"
                        android:src="@drawable/account_settings_logout"
                        app:layout_constraintEnd_toEndOf="parent"
                        cardView:layout_constraintBottom_toBottomOf="parent"
                        cardView:layout_constraintLeft_toRightOf="@id/Synchronization2"
                        cardView:layout_constraintTop_toTopOf="parent" />

                    <TextView
                        android:id="@+id/Synchronization2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="8dp"
                        android:layout_marginStart="8dp"
                        android:layout_marginTop="8dp"
                        android:gravity="center_vertical"
                        android:lines="1"
                        android:padding="5dp"
                        android:text="Logout"
                        android:textColor="#000000"
                        android:textStyle="bold"
                        cardView:layout_constraintStart_toStartOf="parent"
                        cardView:layout_constraintTop_toTopOf="parent" />
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        cardView:srcCompat="@drawable/go_back_arrow" />

</androidx.constraintlayout.widget.ConstraintLayout>
